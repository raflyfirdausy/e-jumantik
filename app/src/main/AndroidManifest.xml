        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".User.DetailInputanActivity"></activity>
        <activity android:name=".Activity.UserMainActivity" />
